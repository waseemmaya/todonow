{
  "expo": {
    "name": "ToDoNow",
    "slug": "ToDoNow",
    "privacy": "public",
    "sdkVersion": "31.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.mayatech.todonow",
      "supportsTablet": true
    },
    "android": {
      "package": "com.mayatech.todonow"
    },
    "description": "A Simple To Do App"
  }
}